{
    "name": "with-electron",
    "description": "Electron + Create React App + Electron Builder",
    "version": "0.1.0",
    "private": true,
    "author": {
        "name": "Luiz de Queiroz",
        "email": "oluizdequeiroz@gmail.com",
        "url": "https://github.com/luizdequeiroz"
    },
    "build": {
        "appId": "luizdequeiroz/com.github//:https",
        "productName": "WithElectron",
        "copyright": "Copyright Â© 2019 ${author}",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        }
    },
    "dependencies": {
        "electron-is-dev": "^1.0.1",
        "react": "^16.8.3",
        "react-dom": "^16.8.3"
    },
    "homepage": "./",
    "main": "public/electron.js",
    "scripts": {
        "start": "rescripts start",
        "build": "rescripts build",
        "test": "rescripts test",
        "eject": "react-scripts eject",
        "electron": "electron .",
        "electron-dev": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
        "postinstall": "electron-builder install-app-deps",
        "preelectron-pack": "npm run build",
        "electron-pack": "build --win"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@rescripts/cli": "0.0.13",
        "@rescripts/rescript-env": "0.0.11",
        "concurrently": "^4.1.0",
        "electron": "^4.0.6",
        "electron-builder": "^20.38.5",
        "react-scripts": "^2.1.5",
        "typescript": "^3.3.3333",
        "wait-on": "^3.2.0"
    }
}